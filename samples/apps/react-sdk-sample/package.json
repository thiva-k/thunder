{
  "name": "react-sdk-sample",
  "private": true,
  "version": "0.14.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run copy:certs",
    "copy:certs": "test -f server.key && test -f server.cert && cp -f server.key server.cert dist/ || echo 'Warning: SSL certificates not found, skipping copy'",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@asgardeo/react": "0.6.15",
    "@wso2/oxygen-ui": "0.0.1-alpha.9",
    "jwt-decode": "4.0.0",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@eslint/js": "9.39.0",
    "@types/node": "20.19.24",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-basic-ssl": "2.1.0",
    "@vitejs/plugin-react": "5.1.0",
    "eslint": "9.39.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "npm:rolldown-vite@7.1.20"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.20"
    }
  }
}
