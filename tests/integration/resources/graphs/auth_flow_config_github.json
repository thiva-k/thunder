{
    "id": "auth_flow_config_github",
    "type": "AUTHENTICATION",
    "nodes": [
        {
            "id": "start",
            "type": "START",
            "onSuccess": "github_auth"
        },
        {
            "id": "github_auth",
            "type": "TASK_EXECUTION",
            "properties": {
                "idpId": "test-github-idp-id"
            },
            "executor": {
                "name": "GithubOAuthExecutor"
            },
            "onSuccess": "authenticated"
        },
        {
            "id": "authenticated",
            "type": "AUTHENTICATION_SUCCESS"
        }
    ]
}
