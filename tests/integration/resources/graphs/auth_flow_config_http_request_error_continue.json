{
    "id": "auth_flow_config_http_request_error_continue",
    "type": "AUTHENTICATION",
    "nodes": [
        {
            "id": "basic_auth",
            "type": "TASK_EXECUTION",
            "executor": {
                "name": "BasicAuthExecutor"
            },
            "onSuccess": "http_request_error"
        },
        {
            "id": "http_request_error",
            "type": "TASK_EXECUTION",
            "properties": {
                "url": "http://localhost:9091/api/error",
                "method": "POST",
                "headers": "{\"Content-Type\": \"application/json\"}",
                "body": "{\"userId\": \"{{ context.userID }}\"}",
                "errorHandling": "{\"failOnError\": false}",
                "timeout": "5"
            },
            "executor": {
                "name": "HTTPRequestExecutor"
            },
            "onSuccess": "authenticated"
        },
        {
            "id": "authenticated",
            "type": "AUTHENTICATION_SUCCESS"
        }
    ]
}
